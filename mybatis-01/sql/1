CREATE DATABASE mybatis;
USE mybatis;

CREATE TABLE `user`(
`id` INT(20) NOT NULL PRIMARY KEY,
`name` VARCHAR(50) DEFAULT NULL,
`pwd` VARCHAR(50) DEFAULT NULL
)ENGINE=INNODB DEFAULT CHARSET=utf8;

DROP TABLE USER

